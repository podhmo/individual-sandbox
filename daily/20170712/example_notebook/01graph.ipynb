      "source": [
        "%matplotlib inline\n",
        "import matplotlib.pyplot as plt\n",
        "plt.style.use('ggplot')\n",
        "\n",
        "xs = list(range(10))\n",
        "ys = [x * x for x in xs]\n",
        "plt.plot(xs, ys, 'g', label='x ** 2')\n",
        "plt.plot(xs, xs, 'b', label='x')\n",
        "plt.legend()"
      ]
    }
  ],
