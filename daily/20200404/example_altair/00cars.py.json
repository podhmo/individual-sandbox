{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.8.1.json",
  "config": {
    "view": {
      "continuousHeight": 300,
      "continuousWidth": 400
    }
  },
  "data": {
    "url": "https://vega.github.io/vega-datasets/data/cars.json"
  },
  "encoding": {
    "x": {
      "field": "Acceleration",
      "type": "quantitative"
    },
    "y": {
      "field": "Horsepower",
      "type": "quantitative"
    }
  },
  "mark": "point"
}
